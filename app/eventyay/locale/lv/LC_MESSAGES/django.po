#: plugins/sendmail/templates/pretixplugins/sendmail/outbox_form.html:145
msgid "Discard"
msgstr "Atmest"

#: cfp/templates/cfp/event/user_submission_edit.html:143
msgid "Cancel proposal"
msgstr "Atcelt priekšlikumu"

#: cfp/templates/cfp/event/user_submission_edit.html:145
